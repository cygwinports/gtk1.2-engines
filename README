  /usr/share/doc/gtk-engines-0.12/ChangeLog
  /usr/share/doc/gtk-engines-0.12/COPYING
  /usr/share/doc/gtk-engines-0.12/INSTALL
  /usr/share/doc/gtk-engines-0.12/NEWS
  /usr/share/doc/gtk-engines-0.12/README
  /usr/share/themes/Metal/gtk/gtkrc
  /usr/share/themes/Metal/ICON.png
  /usr/share/themes/Metal/README.html
  /usr/share/themes/Notif/gtk/gtkrc
  /usr/share/themes/Notif/ICON.png
