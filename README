  /usr/share/themes/Raleigh/gtk/gtkrc
  /usr/share/themes/Raleigh/ICON.png
  /usr/share/themes/Raleigh/README.html
  /usr/share/themes/Redmond95/gtk/gtkrc
  /usr/share/themes/Redmond95/ICON.png
  /usr/share/themes/Redmond95/README.html

